{
	"compilerOptions": {
			"outDir": "./dist/",
			"target": "es5",
			"lib": ["es2019", "dom", "DOM.Iterable"],
			"allowJs": true,
			"jsx": "react",
			"module": "commonjs",
			"removeComments": true,
			"noImplicitAny": true,
			"strictNullChecks": true,
			"esModuleInterop": true,
			"strict": true,
			"sourceMap": true,
			"declaration": false,
			"skipLibCheck": true,
			"forceConsistentCasingInFileNames": true,
			"moduleResolution": "node",
			"resolveJsonModule": true,
			"isolatedModules": true,
			"baseUrl": "./",
			"paths": {
					"~/*": ["src/*"],
					"~c/*": ["src/components/*"],
					"~s/*": ["src/styles/*"],
					"~sc/*": ["src/scenes/*"],
					"~a/*": ["src/assets/*"],
					"~api/*": ["src/api/*"],
			},
	}
}